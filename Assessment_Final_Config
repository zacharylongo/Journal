set firewall name WAN-2-DMZ rule 20 destination port '22'
set firewall name WAN-2-DMZ rule 20 protocol 'tcp'
set firewall name WAN-2-LAN default-action 'drop'
set firewall name WAN-2-LAN enable-default-log
